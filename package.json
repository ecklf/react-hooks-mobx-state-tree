{
  "name": "react-hooks-mobx-state-tree",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.20",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "mobx": "5.14.2",
    "mobx-react-lite": "1.5.0",
    "mobx-state-tree": "3.15.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-github-btn": "1.1.1",
    "react-scripts": "3.2.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "yarn run build:css && react-scripts start",
    "build": "yarn run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/css/index.src.css -o src/css/index.css",
    "watch:css": "postcss src/css/index.src.css -o src/css/index.css -w",
    "dev": "concurrently \"yarn watch:css --silent\" \"yarn start --silent\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.7.0",
    "concurrently": "5.0.0",
    "cssnano": "4.1.10",
    "postcss-cli": "6.1.3",
    "tailwindcss": "1.1.4"
  }
